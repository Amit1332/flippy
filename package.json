{
    "name": "flippy-seven",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:8000",
    "scripts": {
        "build": "npm run clean-dist && cross-env NODE_ENV=production webpack --mode production --config=config/webpack/webpack.prod.config.ts",
        "clean-dist": "rimraf dist/*",
        "start": "npm run start:dev",
        "start:dev": "cross-env NODE_ENV=development webpack serve --config=config/webpack/webpack.dev.config.ts",
        "dev": "docker-compose -f docker-compose.dev.yml up",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prepare": "husky install",
        "lint": "eslint './src/**/*.{js,ts,tsx}' --quiet"
    },
